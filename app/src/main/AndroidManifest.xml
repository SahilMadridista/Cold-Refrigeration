<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coldrefrigeration">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ColdRefrigeration">
        <activity android:name=".WorkHistoryActivity"></activity>
        <activity android:name=".PendingWorkActivity" />
        <activity android:name=".AssignWorkerActivity" />
        <activity android:name=".Worker.WorkerProfileActivity" />
        <activity android:name=".Admin.AdminProfileActivity" />
        <activity android:name=".Admin.ServicesActivity" />
        <activity android:name=".Worker.WorkerHomePage" />
        <activity android:name=".Admin.AdminHomePage" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.SEND_SMS" />

</manifest>